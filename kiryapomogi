#include <stdio.h>
#define N 20 


int max (int * arr[], int n )
{

int m ;

for( int i = 0; i < n ; i++  )
{

if( arr[i] > m  )
{
         m = arr[i];
}

}
return m ;

}




int main()
{

        int arr[N] ;
        char c ;
        int n = 0 ;

        for( n ; n < N ; n++)
        {

                scanf("%d%c", &arr[n], &c );

                if ( c == '\n' )
        {
                ++n ;
                break;
        }
        }

         //
int *arr[n];
*arr[n] = &arr[n];

int m = max( arr[n],n );

printf("%d",m);
         //

return 0 ;
         
}

