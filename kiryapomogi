#include <stdio.h>
#define N 10


int max (int arr[], int n )
{



for( int i = 0; i < n ; i++  )
{
int m_v ; 

if( arr[i] > m_v  )
{
         m_v = arr[i]; 
}

}
return m_v ; 

}


